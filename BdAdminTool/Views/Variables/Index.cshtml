@model IEnumerable<WebAdminbdTool.Models.Variables>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NombreVariable)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaInicial)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaFinal)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NombreVariable)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaInicial)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaFinal)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdVariable">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdVariable">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdVariable">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
